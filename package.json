{
  "name": "react-query-cache-inspector",
  "displayName": "react-query-cache-inspector",
  "description": "VS code extension to visualize React Query cache",
  "version": "0.0.1",
  "publisher": "menuk-fernando",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/builtbymenuk/react-query-cache-inspector"
  },
  "homepage": "https://github.com/builtbymenuk/react-query-cache-inspector#readme",
  "engines": {
    "vscode": "^1.105.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "reactQueryCacheContainer",
          "title": "React Query",
          "icon": "media/icon.png"
        }
      ]
    },
    "views": {
      "reactQueryCacheContainer": [
        {
          "id": "reactQueryCacheView",
          "name": "Cache Inspector"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "watch": "node esbuild.js --watch",
    "compile": "node esbuild.js",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "npm run check-types && npm run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/vscode": "^1.105.0",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "esbuild": "^0.25.10",
    "eslint": "^9.36.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "ws": "^8.18.3"
  }
}
