{
  "manifest_version": 3,
  "name": "React Query Cache Extractor",
  "version": "1.0.0",
  "description": "Extracts React Query cache from webpages and streams to VS Code.",
  "permissions": ["scripting", "tabs", "activeTab"],
  "host_permissions": ["http://localhost/*", "https://*/*"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["http://localhost/*", "https://*/*"],
      "js": ["contentScript.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["inject.js"],
      "matches": ["<all_urls>"]
    }
  ],
  "icons": {
    "128": "icons/icon128.png"
  }
}
